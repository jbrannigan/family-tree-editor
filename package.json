{
  "name": "family-tree-editor",
  "version": "1.2.0",
  "homepage": ".",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e:install": "playwright install --with-deps",
    "test:all": "npm run test:unit && npm run test:e2e",
    "format": "prettier --write .",
    "format:check": "prettier -c .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:unit": "vitest run --reporter=dot",
    "test:e2e": "playwright test",
    "prepare": "husky",
    "depgraph": "cruiser --exclude node_modules --output graph.html src",
    "dep:dot": "depcruise --include-only '^src' --output-type dot src > depgraph.dot",
    "dep:svg": "depcruise --include-only '^src' --output-type dot src | npx dot -Tsvg -o depgraph.svg",
    "callgraph": "node scripts/build-callgraph.mjs"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-scripts": "^5.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/parser": "^7.28.3",
    "@babel/traverse": "^7.28.3",
    "@eslint/js": "^9.37.0",
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.8.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^18.3.7",
    "@vitest/coverage-v8": "^3.2.4",
    "cruiser": "^1.0.0",
    "dependency": "^0.0.1",
    "dependency-cruiser": "^17.1.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "glob": "^11.0.3",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "playwright": "^1.55.0",
    "playwright-core": "^1.56.1",
    "prettier": "^3.6.2",
    "serve": "^14.2.5",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748",
  "lint-staged": {
    "src/**/*.{js,jsx,css,json,md}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "overrides": {
    "resolve-url-loader": "^5.0.0",
    "react-scripts": {
      "resolve-url-loader": "^5.0.0"
    }
  }
}
